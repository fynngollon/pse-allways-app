<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".AllWaysApp"
        android:allowBackup="true"
        android:icon="@mipmap/allways_app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/allways_app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AllWays">
        <activity
            android:name="com.fynng.allways.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.AllWays">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.pseteamtwo.allways.AllWaysActivity"
            android:exported="true"
            android:theme="@style/Theme.AllWays">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>